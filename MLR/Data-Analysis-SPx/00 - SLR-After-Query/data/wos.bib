Title = {Less Is More: Similarity Models for Content-Based Video Retrieval},
Booktitle = {MULTIMEDIA MODELING, MMM 2023, PT II},
Series = {Lecture Notes in Computer Science},
Year = {2023},
Volume = {13834},
